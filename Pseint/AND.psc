Algoritmo AND
	Escribir "TABLA DE VERDAD - AND (Y)";
	Escribir "-------------------------------------";
	Definir A,B como logico;
	A<-Falso;
	B <- Falso;
	Escribir "A = FALSO, B = FALSO, A Y B = ", B y A ;
	B<-Verdadero;
	Escribir "A = FALSO, B = VERDADERO, A Y B = ", B y A ;
	A<-Verdadero;
	B<-Verdadero;
Escribir "A = VERDADERO, B = VERDADERO, A Y B = ",B y A ;
FinAlgoritmo
	